   "metadata": {},
   "outputs": [],
   "source": [
    "#拿到X和y\n",
    "X=np.c_[data2[:,0:2]]\n",
    "y=np.c_[data2[:,2]]"
   ]
  },
  {
   "cell_type": "code",
